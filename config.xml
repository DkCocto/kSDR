<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>3000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>59</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>1536</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <basebandFilter>2500000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>16</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>500000</deviceSamplingRate>
            <gain>50</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>2677686</startFreq>
        <lastSelectedFreq>4493663</lastSelectedFreq>
        <volume>1.239</volume>
        <filterWidth>3200</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>2</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>0</frequencyShift>
        <Agc>
            <threshold>0.075000003087334344</threshold>
            <atackSpeedMs>0.29999998956918716</atackSpeedMs>
            <holdingTimeMs>800</holdingTimeMs>
            <releaseSpeed>5.0000000000000002e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-97.935333</min>
        <max>24.981972</max>
        <speed>2</speed>
    </Waterfall>
    <Spectre>
        <ratio>39.406036</ratio>
        <min>-95.359398</min>
        <speed>5</speed>
        <speed2>2</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <startBin>92253</startBin>
        <stopBin>98300</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>2</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.93900001</bottomCoeff>
        <topCoeff>1.077</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.094999999</decaySpeed>
        <decaySpeedDelta>0.91799998</decaySpeedDelta>
        <smoothingDepth>2</smoothingDepth>
        <spectreCorrectionDb>-106</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4284117265</spectreFillColor>
        <spectreProfileColor>4294967295</spectreProfileColor>
        <mainBGColor>4288782753</mainBGColor>
        <windowsBGColor>4278190080</windowsBGColor>
        <windowsTitleBGColor>4294967295</windowsTitleBGColor>
        <receiveRegionColor>1571726618</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>7055</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>10465</desc>
            <freq>10465000</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
