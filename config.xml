<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>0</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>59</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>600</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>4</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>10</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>2500000</deviceSamplingRate>
            <gain>165</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>13950000</startFreq>
        <lastSelectedFreq>14135000</lastSelectedFreq>
        <volume>2.378</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>0</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>-110</frequencyShift>
        <Agc>
            <threshold>0.10000000311993062</threshold>
            <atackSpeedMs>0.12000000067055228</atackSpeedMs>
            <holdingTimeMs>800</holdingTimeMs>
            <releaseSpeed>0.00061999998938990766</releaseSpeed>
            <lastAmp>150</lastAmp>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-148.12451</min>
        <max>-78.221695</max>
        <speed>1</speed>
    </Waterfall>
    <Spectre>
        <ratio>-62.025253</ratio>
        <min>-152.32095</min>
        <speed>2</speed>
        <speed2>10</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <visibleStartFreq>14065784</visibleStartFreq>
        <visibleStopFreq>14234302</visibleStopFreq>
        <startBin>67433</startBin>
        <stopBin>70193</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.90200001</bottomCoeff>
        <topCoeff>0.92900002</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.00593</decaySpeed>
        <decaySpeedDelta>1.5369999</decaySpeedDelta>
        <smoothingDepth>3</smoothingDepth>
        <spectreCorrectionDb>20</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4283958105</spectreFillColor>
        <spectreProfileColor>4294508433</spectreProfileColor>
        <mainBGColor>4294967295</mainBGColor>
        <windowsBGColor>4279062283</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
        <receiveRegionColor>1660938102</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>10mhz boltovnya</desc>
            <freq>10454900</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>7.055.000</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>80m</desc>
            <freq>3657000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
