<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>0</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>51</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>1536</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>22</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>2500000</deviceSamplingRate>
            <gain>165</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>6855000</startFreq>
        <lastSelectedFreq>7100000</lastSelectedFreq>
        <volume>0.79400003</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>1</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>52434810</frequencyShift>
        <Agc>
            <threshold>0.05000000074505806</threshold>
            <atackSpeedMs>0.15000000000000002</atackSpeedMs>
            <holdingTimeMs>999.99999998509884</holdingTimeMs>
            <releaseSpeed>4.0000004041939983e-05</releaseSpeed>
            <lastAmp>10.988408088684082</lastAmp>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-136.20941</min>
        <max>-66.325142</max>
        <speed>2</speed>
    </Waterfall>
    <Spectre>
        <ratio>-36.201103</ratio>
        <min>-149.33345</min>
        <speed>5</speed>
        <speed2>9</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <visibleStartFreq>7029621</visibleStartFreq>
        <visibleStopFreq>7169941</visibleStopFreq>
        <startBin>68397</startBin>
        <stopBin>70695</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.90100002</bottomCoeff>
        <topCoeff>1.097</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.027000001</decaySpeed>
        <decaySpeedDelta>1.15</decaySpeedDelta>
        <smoothingDepth>2</smoothingDepth>
        <spectreCorrectionDb>-116</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4291791450</spectreFillColor>
        <spectreProfileColor>4294967295</spectreProfileColor>
        <mainBGColor>4278190080</mainBGColor>
        <windowsBGColor>4278190080</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
        <receiveRegionColor>1655028244</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>10mhz boltovnya</desc>
            <freq>10454900</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>7.055.000</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
