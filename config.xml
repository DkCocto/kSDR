<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <deviceDecimationFactor>2</deviceDecimationFactor>
            <gain>54</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>1536</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <basebandFilter>3500000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>2</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>15</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>500000</deviceSamplingRate>
            <gain>50</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>6900000</startFreq>
        <lastSelectedFreq>7063000</lastSelectedFreq>
        <volume>1.202</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>1</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>-30</frequencyShift>
        <Agc>
            <threshold>0.069000002802349636</threshold>
            <atackSpeedMs>0.19999998807907104</atackSpeedMs>
            <holdingTimeMs>1000</holdingTimeMs>
            <releaseSpeed>7.9999999242136251e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-112.53182</min>
        <max>3.3896027</max>
        <speed>2</speed>
    </Waterfall>
    <Spectre>
        <ratio>23.526264</ratio>
        <min>-115.66849</min>
        <speed>5</speed>
        <speed2>2</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <startBin>67702</startBin>
        <stopBin>69976</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.93199998</bottomCoeff>
        <topCoeff>1.123</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.068999998</decaySpeed>
        <decaySpeedDelta>1.3789999</decaySpeedDelta>
        <smoothingDepth>3</smoothingDepth>
        <spectreCorrectionDb>-124</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4283632407</spectreFillColor>
        <spectreProfileColor>4294967295</spectreProfileColor>
        <mainBGColor>4280821800</mainBGColor>
        <windowsBGColor>4278190080</windowsBGColor>
        <windowsTitleBGColor>4294967295</windowsTitleBGColor>
        <receiveRegionColor>1571726618</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>7055</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>10465</desc>
            <freq>10465000</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>myfreq1</desc>
            <freq>7098500</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
