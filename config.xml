<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>6000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>59</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>4000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>2</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>11</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>3200000</deviceSamplingRate>
            <gain>-10</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>3356891</startFreq>
        <lastSelectedFreq>3671200</lastSelectedFreq>
        <volume>1.2309999</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>1</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>100000000</frequencyShift>
        <Agc>
            <threshold>0.060000001220032573</threshold>
            <atackSpeedMs>1</atackSpeedMs>
            <holdingTimeMs>500</holdingTimeMs>
            <releaseSpeed>5.0000000000000002e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-104.03674</min>
        <max>-44.574654</max>
    </Waterfall>
    <Spectre>
        <ratio>-20.447361</ratio>
        <min>-107.18524</min>
        <speed>14</speed>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>131072</fftLen>
        <startBin>36178</startBin>
        <stopBin>39851</stopBin>
        <removeDCBias>1</removeDCBias>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4286069305</spectreFillColor>
        <spectreProfileColor>4290756800</spectreProfileColor>
        <mainBGColor>4281216558</mainBGColor>
        <windowsBGColor>4281015574</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
    </ColorTheme>
</Config>
