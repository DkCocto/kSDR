<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>4000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>58</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>5000</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>5000000</deviceSamplingRate>
            <basebandFilter>2500000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>15</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>3200000</deviceSamplingRate>
            <gain>-10</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>6900000</startFreq>
        <lastSelectedFreq>7131000</lastSelectedFreq>
        <volume>1.174</volume>
        <filterWidth>3200</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>1</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>100000000</frequencyShift>
        <Agc>
            <threshold>0.060000001220032573</threshold>
            <atackSpeedMs>0.15000000000000002</atackSpeedMs>
            <holdingTimeMs>999.99999998509884</holdingTimeMs>
            <releaseSpeed>5.0000003789318735e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-126.93779</min>
        <max>-64.046936</max>
        <speed>2</speed>
    </Waterfall>
    <Spectre>
        <ratio>-35.186729</ratio>
        <min>-138.798</min>
        <speed>5</speed>
        <speed2>14</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>131072</fftLen>
        <visibleStartFreq>7053808</visibleStartFreq>
        <visibleStopFreq>7172827</visibleStopFreq>
        <startBin>34784</startBin>
        <stopBin>36343</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.935</bottomCoeff>
        <topCoeff>1.073</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.034000002</decaySpeed>
        <decaySpeedDelta>0.884</decaySpeedDelta>
        <smoothingDepth>1</smoothingDepth>
        <spectreCorrectionDb>-121</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4281337632</spectreFillColor>
        <spectreProfileColor>4291151301</spectreProfileColor>
        <mainBGColor>4282324747</mainBGColor>
        <windowsBGColor>4278190080</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
        <receiveRegionColor>1655028244</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>10mhz boltovnya</desc>
            <freq>10454900</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
