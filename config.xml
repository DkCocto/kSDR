<Config>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		-->
        <type>1</type>
        <decimation>2</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation -->
            <!-- Example: -->
            <!--	deviceSamplingRate = 2000000 -->
            <!--	deviceDecimationFactor = 2 -->
            <!--	decimation = 2 -->
            <!--	Total sampling rate = (2000000 / 2) / 2 = 500000 -->
            <deviceSamplingRate>4000000</deviceSamplingRate>
            <deviceDecimationFactor>2</deviceDecimationFactor>
            <gain>50</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>2</api>
            <disableLna>1</disableLna>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>4000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>14</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
    </Device>
    <Receiver>
        <startFreq>7100000</startFreq>
        <lastSelectedFreq>7085000</lastSelectedFreq>
        <volume>1.179</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2 -->
        <modulation>1</modulation>
    </Receiver>
    <Waterfall>
        <min>-114.60159</min>
        <max>-38.950809</max>
    </Waterfall>
    <Spectre>
        <ratio>-22.200047</ratio>
        <min>-118.0822</min>
        <speed>11</speed>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>32</fftLen>
        <startBin>7560</startBin>
        <stopBin>8785</stopBin>
    </Spectre>
</Config>
