<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>50</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>1536</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>8000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>12</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>2500000</deviceSamplingRate>
            <gain>165</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>17900000</startFreq>
        <lastSelectedFreq>18100000</lastSelectedFreq>
        <volume>0.39899999</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>0</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>-110</frequencyShift>
        <Agc>
            <threshold>0.03500000003259629</threshold>
            <atackSpeedMs>0.12000000067055228</atackSpeedMs>
            <holdingTimeMs>999.99999998360875</holdingTimeMs>
            <releaseSpeed>0.00015000000126310633</releaseSpeed>
            <lastAmp>12.316548347473145</lastAmp>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-121.9241</min>
        <max>-37.199524</max>
        <speed>1</speed>
    </Waterfall>
    <Spectre>
        <ratio>-18.364819</ratio>
        <min>-128.7588</min>
        <speed>2</speed>
        <speed2>10</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <visibleStartFreq>18008216</visibleStartFreq>
        <visibleStopFreq>18191870</visibleStopFreq>
        <startBin>67309</startBin>
        <stopBin>70317</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.90200001</bottomCoeff>
        <topCoeff>0.92900002</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.0090699997</decaySpeed>
        <decaySpeedDelta>2.365</decaySpeedDelta>
        <smoothingDepth>3</smoothingDepth>
        <spectreCorrectionDb>0</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4288286881</spectreFillColor>
        <spectreProfileColor>4294967295</spectreProfileColor>
        <mainBGColor>4278190080</mainBGColor>
        <windowsBGColor>4281140790</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
        <receiveRegionColor>1647366945</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>10mhz boltovnya</desc>
            <freq>10454900</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>7.055.000</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>80m</desc>
            <freq>3657000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
