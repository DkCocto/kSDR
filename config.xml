<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>3000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>59</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
            <basebandFilter>1536</basebandFilter>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>5000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>3</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>15</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>500000</deviceSamplingRate>
            <gain>50</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>6755000</startFreq>
        <lastSelectedFreq>7055595</lastSelectedFreq>
        <volume>1.686</volume>
        <filterWidth>2700</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>1</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>0</frequencyShift>
        <Agc>
            <threshold>0.065000002612359831</threshold>
            <atackSpeedMs>0.19999998807907104</atackSpeedMs>
            <holdingTimeMs>800</holdingTimeMs>
            <releaseSpeed>5.0000000000000002e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-117.85835</min>
        <max>-38.150566</max>
        <speed>2</speed>
    </Waterfall>
    <Spectre>
        <ratio>-11.770126</ratio>
        <min>-127.23878</min>
        <speed>5</speed>
        <speed2>2</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <startBin>71465</startBin>
        <stopBin>75335</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>1.113</bottomCoeff>
        <topCoeff>0.87800002</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.066</decaySpeed>
        <decaySpeedDelta>1.05</decaySpeedDelta>
        <smoothingDepth>2</smoothingDepth>
        <spectreCorrectionDb>-119</spectreCorrectionDb>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4285274121</spectreFillColor>
        <spectreProfileColor>4294967295</spectreProfileColor>
        <mainBGColor>4280354564</mainBGColor>
        <windowsBGColor>4279176978</windowsBGColor>
        <windowsTitleBGColor>4294967295</windowsTitleBGColor>
        <receiveRegionColor>1566551873</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>7055</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>10465</desc>
            <freq>10465000</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
    </Memory>
</Config>
