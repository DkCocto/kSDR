<Config>
    <App>
        <windowWidth>2560</windowWidth>
        <windowHeight>1351</windowHeight>
    </App>
    <Device>
        <!-- 
		Device codes
		0: RSP1/RSP1A
		1: hackrf
		2: rtlsdr
		-->
        <type>1</type>
        <decimation>1</decimation>
        <RSP>
            <!-- Total sampling rate = (deviceSamplingRate / deviceDecimationFactor) / decimation
             Example:
            	deviceSamplingRate = 2000000
            	deviceDecimationFactor = 2
            	decimation = 2
            	Total sampling rate = (2000000 / 2) / 2 = 500000 
			-->
            <deviceSamplingRate>4000000</deviceSamplingRate>
            <deviceDecimationFactor>1</deviceDecimationFactor>
            <gain>53</gain>
            <!-- Api can be 2 (v2) and 3 (v3)-->
            <api>3</api>
            <disableLna>1</disableLna>
        </RSP>
        <HackRF>
            <!-- App sampling rate will be: deviceSamplingRate / decimation -->
            <deviceSamplingRate>5000000</deviceSamplingRate>
            <basebandFilter>1750000</basebandFilter>
            <rxAmp>0</rxAmp>
            <!-- 0 1 2 3 4-->
            <lnaGain>2</lnaGain>
            <!-- 0 .. 30 -->
            <vgaGain>13</vgaGain>
            <!-- 0 .. 40 step 2-->
            <txAmp>20</txAmp>
        </HackRF>
        <RTL>
            <deviceSamplingRate>3200000</deviceSamplingRate>
            <gain>-10</gain>
        </RTL>
    </Device>
    <Receiver>
        <startFreq>10360000</startFreq>
        <lastSelectedFreq>10460000</lastSelectedFreq>
        <volume>1.247</volume>
        <filterWidth>5779</filterWidth>
        <!-- USB:0, LSB:1, AM:2, nFM:3 -->
        <modulation>0</modulation>
        <enableFrequencyShift>0</enableFrequencyShift>
        <frequencyShift>100000000</frequencyShift>
        <Agc>
            <threshold>0.060000001220032573</threshold>
            <atackSpeedMs>0.21000000298023225</atackSpeedMs>
            <holdingTimeMs>800</holdingTimeMs>
            <releaseSpeed>3.0000004294561232e-05</releaseSpeed>
        </Agc>
    </Receiver>
    <Waterfall>
        <min>-105.08511</min>
        <max>-40.678059</max>
    </Waterfall>
    <Spectre>
        <ratio>-23.322445</ratio>
        <min>-117.68726</min>
        <speed>5</speed>
        <speed2>4</speed2>
        <!-- Must be a power of 2. The total will be fftLen = fftLen * 1024 -->
        <fftLen>262144</fftLen>
        <startBin>66008</startBin>
        <stopBin>70306</stopBin>
        <removeDCBias>1</removeDCBias>
        <style>1</style>
        <contourShowsPower>0</contourShowsPower>
        <bottomCoeff>0.87099999</bottomCoeff>
        <topCoeff>1.215</topCoeff>
        <hangAndDecay>1</hangAndDecay>
        <decaySpeed>0.043000001</decaySpeed>
        <decaySpeedDelta>0.99000001</decaySpeedDelta>
        <smoothingDepth>2</smoothingDepth>
    </Spectre>
    <ColorTheme>
        <spectreFillColor>4279661089</spectreFillColor>
        <spectreProfileColor>4291151301</spectreProfileColor>
        <mainBGColor>4291923064</mainBGColor>
        <windowsBGColor>4280552203</windowsBGColor>
        <windowsTitleBGColor>4284887578</windowsTitleBGColor>
        <receiveRegionColor>1655028244</receiveRegionColor>
    </ColorTheme>
    <Memory>
        <record>
            <desc>UA 7055</desc>
            <freq>7055000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>UA 7080</desc>
            <freq>7080000</freq>
            <modulation>1</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>UA 10460</desc>
            <freq>10460000</freq>
            <modulation>0</modulation>
            <filterWidth>2700</filterWidth>
        </record>
        <record>
            <desc>Polnaya noga</desc>
            <freq>10800000</freq>
            <modulation>0</modulation>
            <filterWidth>3300</filterWidth>
        </record>
    </Memory>
</Config>
